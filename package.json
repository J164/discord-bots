{
  "name": "potatobot",
  "version": "0.2.0",
  "description": "A Discord Music Bot with some extra utilities",
  "author": "J164",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/J164/PotatoBot",
  "repository": "github:J164/PotatoBot",
  "bugs": "https://github.com/J164/PotatoBot/issues",
  "engines": {
    "node": ">=16.14.0"
  },
  "type": "module",
  "dependencies": {
    "@discordjs/opus": "0.7.0",
    "@discordjs/voice": "0.8.0",
    "@napi-rs/canvas": "0.1.21",
    "discord.js": "13.6.0",
    "fuse.js": "6.5.3",
    "mongodb": "4.4.1",
    "node-cron": "3.0.0",
    "tweetnacl": "1.0.3",
    "undici": "4.16.0",
    "ytpl": "2.3.0",
    "ytsr": "3.6.0",
    "discord-api-types": "0.30.0",
    "typescript": "4.6.3",
    "@types/node": "17.0.23",
    "@types/node-cron": "3.0.1",
    "pm2": "5.2.0"
  },
  "devDependencies": {
    "@discordjs/rest": "0.3.0",
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@shelf/jest-mongodb": "2.2.1",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "babel-jest": "27.5.1",
    "eslint": "8.12.0",
    "eslint-plugin-unicorn": "41.0.1",
    "jest": "27.5.1",
    "prettier": "2.6.1"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.6",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "5.0.9",
    "zlib-sync": "0.1.7"
  },
  "jest": {
    "clearMocks": true,
    "testTimeout": 300000,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "roots": [
      "test",
      "dist"
    ],
    "collectCoverageFrom": [
      "dist/**/*.js"
    ]
  },
  "scripts": {
    "lint": "eslint ./ --ext .ts && npx prettier --check .",
    "fix": "eslint ./ --ext .ts --fix",
    "build": "tsc",
    "test": "jest",
    "prettier": "npx prettier --write .",
    "deploy": "node deploy.js"
  }
}
