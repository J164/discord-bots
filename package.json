{
  "name": "discord-bots",
  "version": "1.0.0",
  "description": "Discord bot suite with a variety of useful features",
  "author": "J164 <j164e3@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/J164/discord-bots",
  "repository": "github:J164/discord-bots",
  "bugs": "https://github.com/J164/discord-bots/issues",
  "engines": {
    "node": ">=18.12.0"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "0.14.0",
    "discord.js": "14.7.1",
    "mongodb": "4.12.1",
    "node-cron": "3.0.2",
    "pino": "8.7.0",
    "sharp": "0.31.2",
    "sodium-native": "3.4.1"
  },
  "devDependencies": {
    "@discordjs/rest": "1.4.0",
    "@types/node": "18.11.10",
    "@types/node-cron": "3.0.6",
    "@types/sharp": "0.31.0",
    "typescript": "4.9.4",
    "xo": "0.53.1"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.7",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "5.0.10",
    "zlib-sync": "0.1.7"
  },
  "scripts": {
    "lint": "xo",
    "build": "tsc",
    "deploy": "node dist/deploy.js"
  },
  "xo": {
    "prettier": true,
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:unicorn/recommended"
    ],
    "rules": {
      "default-case": "off",
      "getter-return": "off",
      "max-params": "off",
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/no-implicit-any-catch": "off",
      "n/file-extension-in-import": "off"
    }
  }
}
