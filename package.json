{
  "name": "potato-bot",
  "version": "1.0.0",
  "description": "A Discord Music Bot with some extra utilities",
  "author": "J164 <43323130+J164@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/J164/PotatoBot",
  "repository": "github:J164/PotatoBot",
  "bugs": "https://github.com/J164/PotatoBot/issues",
  "engines": {
    "node": ">=18.2.0"
  },
  "type": "module",
  "dependencies": {
    "@discordjs/opus": "0.8.0",
    "@discordjs/rest": "1.3.0",
    "@discordjs/voice": "0.13.0",
    "@types/node": "18.8.3",
    "@types/node-cron": "3.0.4",
    "@types/sharp": "0.31.0",
    "discord-api-types": "0.37.12",
    "discord.js": "14.5.0",
    "dotenv": "16.0.3",
    "mongodb": "4.10.0",
    "node-cron": "3.0.2",
    "pino": "8.6.1",
    "pino-pretty": "9.1.1",
    "sharp": "0.31.1",
    "sodium-native": "3.4.1",
    "typescript": "4.7.4",
    "ytpl": "2.3.0",
    "ytsr": "3.8.0"
  },
  "devDependencies": {
    "xo": "0.52.4"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.6",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "5.0.9",
    "zlib-sync": "0.1.7"
  },
  "scripts": {
    "lint": "xo",
    "build": "tsc",
    "deploy": "node dist/deploy.js"
  },
  "xo": {
    "prettier": true,
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:unicorn/recommended"
    ],
    "rules": {
      "default-case": "off",
      "getter-return": "off",
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/no-implicit-any-catch": "off",
      "n/file-extension-in-import": "off"
    }
  }
}
